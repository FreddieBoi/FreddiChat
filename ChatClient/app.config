<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_IChatService" />
            </netNamedPipeBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IChatService" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="net.pipe://localhost/FreddiChat" binding="netNamedPipeBinding"
                bindingConfiguration="NetNamedPipeBinding_IChatService" contract="ChatServiceReference.IChatService"
                name="NetNamedPipeBinding_IChatService">
            </endpoint>
            <endpoint address="http://localhost:8080/FreddiChat" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IChatService" contract="ChatServiceReference.IChatService"
                name="WSDualHttpBinding_IChatService">
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
